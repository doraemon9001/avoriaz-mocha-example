{
  "name": "avoriaz-mocha-example",
  "version": "1.0.0",
  "description": "example project using avoriaz with mocha-webpack to test Vue.js components",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha-webpack --webpack-config webpack.config.js test --recursive --require test/.setup.js"
  },
  "author": "Edd Yerburgh",
  "license": "MIT",
  "devDependencies": {
    "avoriaz": "^1.0.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "vue": "^2.1.8"
  }
